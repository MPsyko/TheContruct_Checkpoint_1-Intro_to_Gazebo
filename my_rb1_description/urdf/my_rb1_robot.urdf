<?xml version="1.0" ?>

<robot name="rb1_robot">
    <!-- Define materials for visualization -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>
    <material name="gray">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>
    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>
    <!-- Base footprint link -->
    <link name="base_footprint">
    </link>
    <!-- Joint connecting base_link and base_footprint -->
    <joint name="base_link_plate_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 -0.15" />
        <parent link="base_link" />
        <child link="base_footprint" />
    </joint>
    <!-- Base link -->
    <link name="base_link">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.3" radius="0.25"/> 
            </geometry>
            <material name="blue">
                <color rgba="0 0 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.30" radius="0.25"/> 
            </geometry>
        </collision>
        <inertial>
            <mass value="20" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.153125" ixy="0" ixz="0" iyy="0.153125" iyz="0" izz="0.625"/>
        </inertial>
    </link>
    <!-- Left wheel link -->
    <link name="left_wheel">
        <!-- Left wheel visual tag -->
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/> 
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <!-- Left wheel collision tag -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/> 
            </geometry>
        </collision>
        <!-- Left wheel inertial tag -->
        <inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.00135625" ixy="0" ixz="0" iyy="0.00135625" iyz="0" izz="0.0003125"/>
        </inertial>
    </link>
    <!-- Joint connecting base_link and left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <axis xyz="0 1 0" />
        <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
        <parent link="base_link" />
        <child link="left_wheel" />
    </joint>
    <!-- Right wheel link -->
    <link name="right_wheel">
        <!-- Right wheel visual tag -->
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/> 
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <!-- Right wheel collision tag -->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.03" radius="0.025"/> 
            </geometry>
        </collision>
        <!-- Right wheel inertial tag -->
        <inertial>
            <mass value="1" />
            <origin rpy="0 0 0" xyz="0 0 0" />
            <inertia ixx="0.00135625" ixy="0" ixz="0" iyy="0.00135625" iyz="0" izz="0.0003125"/>
        </inertial>
    </link>
    <!-- Joint connecting base_link and right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <axis xyz="0 1 0" />
        <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
        <parent link="base_link" />
        <child link="right_wheel" />
    </joint>
    <!-- Front caster -->
    <link name="front_caster">
        <!-- Front caster visual tag -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="gray">
                <color rgba="0.40 0.40 0.40 1"/>
            </material>
        </visual>
        <!-- Front caster collision tag -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0</mu>
                        <mu2>0</mu2>
                    </ode>
                </friction>
        </surface>
        </collision>
        <!-- Front caster inertial tag -->
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>
    </link>
    <!-- Joint connecting base_link and front_caster -->
    <joint name="base_link_front_caster_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.15" />
        <parent link="base_link" />
        <child link="front_caster" />
    </joint>
    <!-- back caster -->
    <link name="back_caster">
        <!-- Back caster visual tag -->
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="gray">
                <color rgba="0.40 0.40 0.40 1"/>
            </material>
        </visual>
        <!-- Back caster collision tag -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <surface>
                <friction>
                    <ode>
                        <mu>0</mu>
                        <mu2>0</mu2>
                    </ode>
                </friction>
        </surface>
        </collision>
        <!-- Back caster inertial tag -->
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
        </inertial>
    </link>
    <!-- Joint connecting base_link and back_caster -->
    <joint name="base_link_back_caster_joint" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15" />
        <parent link="base_link" />
        <child link="back_caster" />
    </joint>
    <!-- Link representing the front laser scanner -->
    <!--For this robot, the laser is not visible, so we won't define a body for it. -->
    <link name="front_laser">
    </link>
    <!-- Joint connecting base_link and front_laser -->
    <joint name="base_link_front_laser_joint" type="fixed">
        <origin rpy="3.14159 0 0 " xyz="0.25 0.0 0.075" />
        <parent link="base_link" />
        <child link="front_laser" />
    </joint>


    <!-- Gazebo visualization -->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Left Wheel -->
    <gazebo reference="left_wheel">
        <!-- TODO: Define mu1 (first friction coefficient) -->
        <mu1>1</mu1>
        <!-- TODO: Define mu2 (second friction coefficient) -->
        <mu2>1</mu2>
        <!-- TODO: Define kp (contact stiffness) -->
        <kp>1e7</kp>
        <!-- TODO: Define kd (contact damping) -->
        <kd>1</kd>
        <!-- TODO: Define fdir1 (friction direction) -->
        <fdir1>1 0 0</fdir1>
        <!-- TODO: Choose a material for visualization -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- Right Wheel -->
    <gazebo reference="right_wheel">
        <!-- TODO: Define mu1 (first friction coefficient) -->
        <mu1>1</mu1>
        <!-- TODO: Define mu2 (second friction coefficient) -->
        <mu2>1</mu2>
        <!-- TODO: Define kp (contact stiffness) -->
        <kp>1e7</kp>
        <!-- TODO: Define kd (contact damping) -->
        <kd>1</kd>
        <!-- TODO: Define fdir1 (friction direction) -->
        <fdir1>1 0 0</fdir1>
        <!-- TODO: Choose a material for visualization -->
        <material>Gazebo/Blue</material>
    </gazebo>

    <!-- Front Caster -->
    <gazebo reference="front_caster">
        <!-- TODO: Define mu1 (first friction coefficient) -->
        <mu1>0</mu1>
        <!-- TODO: Define mu2 (second friction coefficient) -->
        <mu2>0</mu2>
        <!-- TODO: Define kp (contact stiffness) -->
        <kp>1e7</kp>
        <!-- TODO: Define kd (contact damping) -->
        <kd>1</kd>
        <!-- TODO: Define fdir1 (friction direction) -->
        <fdir1>1 0 0</fdir1>
        <!-- TODO: Choose a material for visualization -->
        <material>Gazebo/Red</material>
    </gazebo>

    <!-- Back Caster -->
    <gazebo reference="back_caster">
        <!-- TODO: Define mu1 (first friction coefficient) -->
        <mu1>0</mu1>
        <!-- TODO: Define mu2 (second friction coefficient) -->
        <mu2>0</mu2>
        <!-- TODO: Define kp (contact stiffness) -->
        <kp>1e7</kp>
        <!-- TODO: Define kd (contact damping) -->
        <kd>1</kd>
        <!-- TODO: Define fdir1 (friction direction) -->
        <fdir1>1 0 0</fdir1>
        <!-- TODO: Choose a material for visualization -->
        <material>Gazebo/Red</material>
    </gazebo>
</robot>